	int print2largest(int a[], int n) {
	    int large=a[0];
	    int slargest=-1;
	    for(int i=0;i<n;i++)
	    {
	        if(a[i]>large)
	          {
	              slargest=large;
	              large=a[i];
	              
	          }
	          else if(a[i]<large&&a[i]>slargest)
	          {
	              slargest=a[i];
	          }
	    }
	  
	  return slargest;
	  
	      
	    
	}

