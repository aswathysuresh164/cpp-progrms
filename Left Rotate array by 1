
#include <bits/stdc++.h>

using namespace std;

int main()
{
    cout<<"Left rotate Array\n";
    int a[]={1,2,3,4,5};
    int n=sizeof(a)/sizeof(a[0]);
    int temp=a[0];
    for(int i=1;i<n;i++)
    {
        a[i-1]=a[i];
    }
    a[n-1]=temp;
    
    cout<<"\n Array after the left rotate : ";
     for(int i=0;i<n;i++)
    {
       cout<<a[i]<<" ";
    }
    

    return 0;
}
