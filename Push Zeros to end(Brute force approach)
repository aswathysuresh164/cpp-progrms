	void pushZerosToEnd(int arr[], int n) {
	    // code here
	    vector<int> v;
	    for(int i=0;i<n;i++)
	    {
	        if(arr[i]!=0)
	          v.push_back(arr[i]);
	          
	    }
	    int i;
	    for(i=0;i<v.size();i++)
	    {
	       
	              arr[i]=v[i];
	         
	    }
	     for(;i<n;i++)
	    {
	        arr[i]=0;
	    }
	      
	    
	}
